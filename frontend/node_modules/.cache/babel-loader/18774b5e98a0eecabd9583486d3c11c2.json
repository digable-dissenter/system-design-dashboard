{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'SignUp',\n\n  data() {\n    return {\n      name: '',\n      email: '',\n      password: ''\n    };\n  },\n\n  methods: {\n    async signUp() {\n      let result = await axios.post(\"http://localhost:3000/users\", {\n        email: this.email,\n        password: this.password,\n        name: this.name\n      });\n      console.warn(result);\n\n      if (result.status == 201) {\n        localStorage.setItem(\"user-info\", JSON.stringify(result.data));\n        this.$router.push({\n          name: 'HomePage'\n        });\n      }\n    }\n\n  },\n\n  mounted() {\n    let user = localStorage.getItem('user-info');\n\n    if (user) {\n      this.$router.push({\n        name: 'HomePage'\n      });\n    }\n  }\n\n};","map":{"version":3,"mappings":"AAaA,OAAOA,KAAP,MAAkB,OAAlB;AAGA,eAAe;EACXC,IAAI,EAAC,QADM;;EAEXC,IAAI,GACJ;IACI,OAAM;MACFD,IAAI,EAAC,EADH;MAEFE,KAAK,EAAC,EAFJ;MAGFC,QAAQ,EAAC;IAHP,CAAN;EAKH,CATU;;EAUXC,OAAO,EAAC;IACJ,MAAMC,MAAN,GACA;MACI,IAAIC,MAAK,GAAI,MAAMP,KAAK,CAACQ,IAAN,CAAW,6BAAX,EAAyC;QACxDL,KAAK,EAAC,KAAKA,KAD6C;QAExDC,QAAQ,EAAC,KAAKA,QAF0C;QAGxDH,IAAI,EAAC,KAAKA;MAH8C,CAAzC,CAAnB;MAMAQ,OAAO,CAACC,IAAR,CAAaH,MAAb;;MAEA,IAAGA,MAAM,CAACI,MAAP,IAAe,GAAlB,EACA;QAEIC,YAAY,CAACC,OAAb,CAAqB,WAArB,EAAiCC,IAAI,CAACC,SAAL,CAAeR,MAAM,CAACL,IAAtB,CAAjC;QACA,KAAKc,OAAL,CAAaC,IAAb,CAAkB;UAAChB,IAAI,EAAC;QAAN,CAAlB;MACJ;IACJ;;EAjBI,CAVG;;EA8BXiB,OAAO,GACP;IACI,IAAIC,IAAI,GAACP,YAAY,CAACQ,OAAb,CAAqB,WAArB,CAAT;;IACA,IAAGD,IAAH,EACA;MACE,KAAKH,OAAL,CAAaC,IAAb,CAAkB;QAAChB,IAAI,EAAC;MAAN,CAAlB;IACF;EACJ;;AArCW,CAAf","names":["axios","name","data","email","password","methods","signUp","result","post","console","warn","status","localStorage","setItem","JSON","stringify","$router","push","mounted","user","getItem"],"sourceRoot":"","sources":["/Users/amina/aifmrm_ers_dashboard/src/components/SignUp.vue"],"sourcesContent":["<template>\n<img class =\"logo\" src=\"../assets/download.png\"/>\n<h1>Sign Up</h1>\n<div class =\"register\">\n    <input type =\"text\" v-model=\"name\" placeholder=\"Enter Name\" />\n    <input type =\"text\" v-model=\"email\" placeholder=\"Enter Email\" />\n    <input type =\"text\" v-model=\"password\" placeholder=\"Enter Password\" />\n    <button v-on:click=\"signUp\"> Sign Up</button>\n</div>\n</template>\n\n<script>\n\nimport axios from 'axios'\n\n\nexport default {\n    name:'SignUp',\n    data()\n    {\n        return{\n            name:'',\n            email:'',\n            password:''\n        }\n    },\n    methods:{\n        async signUp()\n        {\n            let result = await axios.post(\"http://localhost:3000/users\",{\n                email:this.email,\n                password:this.password,\n                name:this.name\n            });\n\n            console.warn(result);\n\n            if(result.status==201)\n            {\n                \n                localStorage.setItem(\"user-info\",JSON.stringify(result.data))\n                this.$router.push({name:'HomePage'})\n            }\n        }\n    },\n\n    mounted()\n    {\n        let user=localStorage.getItem('user-info');\n        if(user)\n        {\n          this.$router.push({name:'HomePage'})   \n        }\n    }\n}\n</script>\n\n<style>\n.logo{\n    width:300px\n}\n\n.register input{\n    width: 300px;\n    height: 40px;\n    padding-left: 20px;\n    display: block;\n    margin-bottom: 30px;\n    margin-right: auto;\n    margin-left: auto;\n    border: 1px solid black;\n\n}\n\n\n.register button {\n    width: 320px;\n    height: 40px;\n    border: 1px solid grey;\n    cursor: pointer;\n\n}\n</style>"]},"metadata":{},"sourceType":"module"}